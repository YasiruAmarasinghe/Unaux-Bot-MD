		"bot"
                "nodejs"
	],
	"repository": "https://github.com/YasiruAmarasinghe/Unaux-Bot-MD",
	"stack": "container",
	"env": {
		"ALIVE_MSG": {
			"description": "Messege content for show with Alive message",
